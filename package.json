{
  "name": "deepnest",
  "version": "1.5.6",
  "description": "Deep nesting for Laser and CNC",
  "private": true,
  "main": "main.js",
  "types": "index.d.ts",
  "license": "MIT",
  "funding": [
    {
      "type": "patreon",
      "url": "https://patreon.com/deepnest_next?utm_medium=clipboard_copy&utm_source=copyLink&utm_campaign=creatorshare_fan&utm_content=join_link"
    }
  ],
  "scripts": {
    "start": "electron .",
    "start:new": "cross-env deepnest_new_ui=1 electron .",
    "start:legacy": "electron .",
    "start:debug": "cross-env deepnest_debug=1 electron .",
    "start:new-debug": "cross-env deepnest_new_ui=1 deepnest_debug=1 electron .",
    "compare:init": "node scripts/feature-comparison.js init",
    "compare:show": "node scripts/feature-comparison.js show",
    "compare:help": "node scripts/feature-comparison.js help",
    "perf:analyze": "node scripts/performance-validation.js analyze",
    "perf:build-time": "node scripts/performance-validation.js build-time",
    "perf:bundle-size": "node scripts/performance-validation.js bundle-size",
    "perf:full": "node scripts/performance-validation.js full",
    "feedback:submit": "node scripts/feedback-collection.js submit",
    "feedback:show": "node scripts/feedback-collection.js show",
    "feedback:export": "node scripts/feedback-collection.js export",
    "feedback:list": "node scripts/feedback-collection.js list",
    "rollout:status": "node scripts/ui-rollout.js status",
    "rollout:phases": "node scripts/ui-rollout.js phases",
    "rollout:enable": "node scripts/ui-rollout.js enable",
    "rollout:disable": "node scripts/ui-rollout.js disable",
    "rollout:advance": "node scripts/ui-rollout.js advance",
    "rollout:rollback": "node scripts/ui-rollout.js rollback",
    "deploy:status": "node scripts/deployment-monitoring.js status",
    "deploy:health": "node scripts/deployment-monitoring.js health",
    "deploy:history": "node scripts/deployment-monitoring.js history",
    "deploy:watch": "node scripts/deployment-monitoring.js watch",
    "deploy:export": "node scripts/deployment-monitoring.js export",
    "printenv": "cross-replace node -e \"console.log(process.env)\"",
    "test": "playwright test",
    "build": "npm run build:backend && npm run build:frontend && electron-rebuild",
    "build:backend": "tsc",
    "build:frontend": "cd frontend-new && npm run build",
    "build:legacy": "tsc && electron-rebuild",
    "clean": "rmdir /s /q build dist main/ui-new >nul 2>&1|echo . >nul && cd frontend-new && npm run clean >nul 2>&1|echo . >nul && cd ..",
    "clean:frontend": "rmdir /s /q main/ui-new >nul 2>&1|echo . >nul && cd frontend-new && npm run clean >nul 2>&1|echo . >nul && cd ..",
    "clean:backend": "rmdir /s /q build >nul 2>&1|echo . >nul",
    "clean-all": "rmdir /s /q build dist main/ui-new node_modules bin >nul 2>&1|echo . >nul && cd frontend-new && rmdir /s /q node_modules dist >nul 2>&1|echo . >nul && cd ..",
    "dist": "cross-replace npx @electron/packager . deepnest-v$npm_package_version --overwrite --ignore='\\.github' --ignore='\\.vscode' --ignore='\\.husky' --ignore='deepnest-v[0-9]+\\.[0-9]+\\.[0-9]+-[a-z0-9_-]' --ignore='out' --ignore='renovate\\.json' --ignore='\\.readthedocs\\.yaml' --ignore='CHANGELOG\\.md' --ignore='examples' --ignore='\\.gitignore' --ignore='playwright-report' --ignore='test.log'",
    "build-dist": "electron-builder build --win",
    "build-dist-signed": "electron-builder build --win --publish never",
    "dist-all": "npm run clean-all && npm install && npm run build && npm run dist",
    "dist-all-signed": "npm run clean-all && npm install && npm run build && npm run build-dist-signed",
    "prepare": "husky || true",
    "precommit": "lint-staged",
    "postinstall": "electron-builder install-app-deps",
    "pw:codegen": "node helper_scripts/playwright_codegen.js",
    "docs:generate": "tsc && jsdoc -c jsdoc.conf.json",
    "docs:serve": "cd docs/api && python -m http.server 8080",
    "docs:markdown": "jsdoc2md --configure ./jsdoc2md.json \"main/**/*.js\" \"build/**/*.js\" > docs/API.md",
    "lint:jsdoc": "eslint --rule \"require-jsdoc: error\" main/",
    "docs:validate": "npm run lint:jsdoc && npm run docs:generate"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "**/*.{ts,html,css,scss,less,json}": [
      "prettier --write",
      "eslint --fix",
      "git add"
    ]
  },
  "repository": "https://github.com/deepnest-next/deepnest",
  "keywords": [
    "Electron",
    "Nesting",
    "CNC",
    "Laser"
  ],
  "devDependencies": {
    "@babel/cli": "^7.28.0",
    "@babel/core": "^7.28.0",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-object-rest-spread": "^7.20.7",
    "@babel/preset-env": "^7.28.0",
    "@babel/preset-typescript": "^7.27.1",
    "@electron/packager": "18.3.6",
    "@electron/rebuild": "4.0.1",
    "@eslint/js": "^9.26.0",
    "@playwright/test": "1.52.0",
    "@tailwindcss/postcss": "^4.1.11",
    "@tailwindcss/vite": "^4.1.11",
    "@types/node": "22.15.17",
    "cross-replace": "0.2.0",
    "electron": "34.5.5",
    "electron-builder": "26.0.15",
    "eslint": "^9.26.0",
    "husky": "^9.1.7",
    "jsdoc-babel": "^0.5.0",
    "jsdoc-to-markdown": "^9.1.1",
    "lint-staged": "^15.5.2",
    "postcss": "^8.5.6",
    "prettier": "^3.5.3",
    "tailwindcss": "^4.1.11",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.32.0"
  },
  "dependencies": {
    "@deepnest/calculate-nfp": "202503.13.155300",
    "@deepnest/svg-preprocessor": "0.2.0",
    "@electron/remote": "2.1.2",
    "axios": "1.9.0",
    "form-data": "4.0.2",
    "graceful-fs": "4.2.11",
    "jsdoc": "^4.0.4",
    "marked": "15.0.11"
  },
  "files": [
    "main.js",
    "main/**/*",
    "index.d.ts",
    "node_modules",
    "package.json",
    "icon.icns",
    "icon.ico"
  ],
  "build": {
    "appId": "net.deepnest.app",
    "copyright": "2025 deepnest-next Community",
    "nodeGypRebuild": false,
    "files": [
      "**/*",
      "build/**/*.js",
      "!**/deepnest-v*",
      "!**/out",
      "!**/playwright-*",
      "!**/test*",
      "!**/*.ts",
      "!**/node_modules/*/{CHANGELOG.md,README.md,README,readme.md,readme}",
      "!**/node_modules/*/{test,__tests__,tests,powered-test,example,examples}",
      "!**/node_modules/*.d.ts",
      "!**/node_modules/.bin",
      "!**/*.{iml,o,hprof,orig,pyc,pyo,rbc,swp,csproj,sln,xproj}",
      "!.editorconfig",
      "!**/package-lock.json",
      "!test**",
      "!.readthedocs.yaml",
      "!**/._*",
      "!{examples,helper_scripts}",
      "!**/{.DS_Store,.git,.hg,.svn,CVS,RCS,SCCS,.gitignore,.gitattributes}",
      "!**/{__pycache__,thumbs.db,.flowconfig,.idea,.vs,.vscode,.husky,.nyc_output}",
      "!**/{appveyor.yml,.travis.yml,circle.yml}",
      "!**/{npm-debug.log,yarn.lock,.yarn-integrity,.yarn-metadata.json}"
    ],
    "directories": {
      "app": ".",
      "output": "deepnest-v${version}-win32-${arch}"
    },
    "win": {
      "target": [
        "dir",
        "zip"
      ],
      "icon": "icon.ico",
      "artifactName": "${name}-v${version}-win32-${arch}.${ext}",
      "signAndEditExecutable": true,
      "forceCodeSigning": true,
      "signExts": [
        ".node",
        ".exe",
        ".dll",
        ".sys",
        ".msi",
        ".appx",
        ".appxbundle"
      ],
      "signtoolOptions": {
        "sign": "./helper_scripts/sign_windows.js",
        "signingHashAlgorithms": [
          "sha256"
        ]
      }
    },
    "mac": {
      "category": "public.app-category.utilities",
      "icon": "icon.icns"
    }
  },
  "contributors": [
    {
      "name": "Josef Fr√∂hle",
      "email": "github@josef-froehle.de"
    }
  ]
}
