      "name": "Josef Fr√∂hle",
      "email": "github@josef-froehle.de",
      "url": "https://www.josef-froehle.de"
    }
  ],
  "scripts": {
    "format": "prettier --write .",
    "test": "playwright test",
    "lint": "eslint . --ext .js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix",
    "typecheck:node": "tsc --noEmit -p tsconfig.node.json --composite false",
    "typecheck:web": "tsc --noEmit -p tsconfig.web.json --composite false",
    "typecheck": "npm run typecheck:node && npm run typecheck:web",
    "start": "electron-vite preview",
    "dev": "electron-vite dev -- --trace-warnings",
    "build": "npm run typecheck && electron-vite build",
    "postinstall": "electron-builder install-app-deps",
    "build:unpack": "npm run build && electron-builder --dir",
    "build:win": "npm run build && electron-builder --win",
    "build:mac": "electron-vite build && electron-builder --mac",
    "build:linux": "electron-vite build && electron-builder --linux"
  },
  "dependencies": {
    "@deepnest/calculate-nfp": "^202412.22.211007",
    "@deepnest/clipper2": "^0.0.1",
    "@deepnest/geometryutil": "^0.0.3",
    "@deepnest/svg-preprocessor": "^0.1.2",
    "@electron-toolkit/preload": "^3.0.1",
    "@electron-toolkit/utils": "^3.0.0",
    "@electron/remote": "^2.1.2",
    "axios": "^1.7.9",
    "electron-updater": "6.3.9",
    "form-data": "^4.0.1",
    "graceful-fs": "^4.2.11"
  },
  "devDependencies": {
    "@electron-toolkit/eslint-config-prettier": "^2.0.0",
    "@electron-toolkit/eslint-config-ts": "^2.0.0",
    "@electron-toolkit/tsconfig": "^1.0.1",
    "@playwright/test": "^1.49.1",
    "@swc/core": "^1.10.7",
    "@types/graceful-fs": "^4.1.9",
    "@types/node": "^22.10.5",
    "autoprefixer": "^10.4.20",
    "d3-polygon": "^3.0.1",
    "electron": "32.2.7",
    "electron-builder": "25.1.8",
    "electron-vite": "^2.3.0",
    "eslint": "^8.57.0",
    "eslint-plugin-solid": "0.14.5",
    "interactjs": "^1.10.27",
    "piscina": "^5.0.0-alpha.0",
    "postcss": "^8.4.49",
    "prettier": "^3.3.2",
    "ractive": "^1.4.4",
    "solid-js": "1.9.4",
    "svg-pan-zoom": "^3.6.2",
    "tailwindcss": "^3.4.17",
    "typescript": "5.7.3",
    "vite": "^6.0.7",
    "vite-plugin-solid": "2.11.0"
  },
  "overrides": {
    "vite": "$vite"
  }
}
