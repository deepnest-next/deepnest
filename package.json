  "scripts": {
    "start": "electron .",
    "forge-start": "electron-forge start",
    "test": "playwright test",
    "build": "electron-rebuild",
    "clean": "rmdir /s /q build dist >nul 2>&1|echo . >nul",
    "clean:out": "rmdir /s /q build dist out >nul 2>&1|echo . >nul",
    "clean-all": "rmdir /s /q build dist node_modules bin out >nul 2>&1|echo . >nul",
    "dist": "cross-replace npx @electron/packager . deepnest-v$npm_package_version --overwrite --ignore='\\.github' --ignore='\\.vscode' --ignore='deepnest-v[0-9]+\\.[0-9]+\\.[0-9]+-[a-z0-9_-]' --ignore='out' --ignore='renovate\\.json' --ignore='\\.readthedocs\\.yaml' --ignore='CHANGELOG\\.md' --ignore='examples' --ignore='\\.gitignore'",
    "dist-all": "npm run clean-all && npm install && npm run build && npm run dist",
    "package": "electron-forge package",
    "postinstall": "patch-package",
    "make": "electron-forge make",
    "publish:dry": "electron-forge publish --dry-run",
    "publish:fromdry": "electron-forge publish --from-dry-run",
    "publish": "electron-forge publish"
  },
  "repository": "https://github.com/deepnest-next/deepnest",
  "keywords": [
    "Electron",
    "Nesting",
    "CNC",
    "Laser"
  ],
