  "author": "deepnest-next contributors",
  "homepage": "https://www.deepnest.net",
  "license": "MIT",
  "funding": [
    {
      "type": "patreon",
      "url": "https://patreon.com/deepnest_next?utm_medium=clipboard_copy&utm_source=copyLink&utm_campaign=creatorshare_fan&utm_content=join_link"
    }
  ],
  "repository": {
    "url": "git+https://github.com/deepnest-next/deepnest.git",
