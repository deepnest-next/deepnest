{
  "name": "deepnest",
  "version": "1.5.1",
  "description": "Deep nesting for Laser and CNC",
  "private": true,
  "main": "main.js",
  "types": "index.d.ts",
  "license": "MIT",
  "author": {
    "name": "deepnest-next Community",
    "email": "stuff@deepnest.net",
    "url": "https://deepnest.net"
  },
  "funding": [
    {
      "type": "patreon",
      "url": "https://patreon.com/deepnest_next?utm_medium=clipboard_copy&utm_source=copyLink&utm_campaign=creatorshare_fan&utm_content=join_link"
    }
