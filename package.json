{
  "name": "deepnest",
  "version": "2.0.0-dev",
  "description": "deep nesting for Laser and CNC",
  "private": true,
  "main": "./out/main/index.js",
  "author": "deepnest-next contributors",
  "homepage": "https://www.deepnest.net",
  "license": "MIT",
  "funding": [
    {
      "type": "patreon",
      "url": "https://patreon.com/deepnest_next?utm_medium=clipboard_copy&utm_source=copyLink&utm_campaign=creatorshare_fan&utm_content=join_link"
    }
  ],
  "repository": {
    "url": "git+https://github.com/deepnest-next/deepnest.git",
    "type": "git"
  },
  "keywords": [
    "Electron",
    "Nesting",
    "CNC",
    "Laser"
  ],
  "contributors": [
    {
      "name": "Josef Fr√∂hle",
      "email": "github@josef-froehle.de",
      "url": "https://www.josef-froehle.de"
    }
  ],
  "scripts": {
    "format": "prettier --write .",
    "test": "playwright test",
    "lint": "eslint . --ext .js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix",
    "typecheck:node": "tsc --noEmit -p tsconfig.node.json --composite false",
    "typecheck:web": "tsc --noEmit -p tsconfig.web.json --composite false",
    "typecheck": "npm run typecheck:node && npm run typecheck:web",
    "start": "electron-vite preview",
    "re-install": "shx rm -rf node_modules && yarn install",
    "clean": "shx rm -rf out node_modules dist deepnest-* test-results playwright-report",
    "dev": "cross-env NODE_ENV=development electron-vite dev --mode development -- --trace-warnings",
    "build": "npm run typecheck && electron-vite build",
    "postinstall": "electron-builder install-app-deps",
    "build:unpack": "npm run build && electron-builder --dir",
    "build:win": "npm run build && electron-builder --win",
    "build:mac": "electron-vite build && electron-builder --mac",
    "build:linux": "electron-vite build && electron-builder --linux"
  },
  "dependencies": {
    "@deepnest/calculate-nfp": "202502.27.164905",
    "@deepnest/clipper2": "link:../node-clipper2",
    "@deepnest/geometryutil": "link:../node-geometryutil",
    "@deepnest/svg-preprocessor": "link:../deepnest-svg-preprocessor",
    "@doodle3d/clipper-js": "^1.0.11",
    "@electron-toolkit/preload": "3.0.1",
    "@electron-toolkit/utils": "4.0.0",
    "@electron/remote": "2.1.2",
    "axios": "1.8.1",
    "electron-updater": "6.6.1",
    "form-data": "4.0.2",
    "graceful-fs": "4.2.11"
  },
  "devDependencies": {
    "@electron-toolkit/eslint-config-prettier": "3.0.0",
    "@electron-toolkit/eslint-config-ts": "3.0.0",
    "@electron-toolkit/tsconfig": "^1.0.1",
    "@playwright/test": "^1.49.1",
    "@swc/core": "^1.10.7",
    "@types/graceful-fs": "^4.1.9",
    "@types/node": "^22.10.5",
    "autoprefixer": "^10.4.20",
    "cross-env": "^7.0.3",
    "d3-polygon": "^3.0.1",
    "electron": "34.3.0",
    "electron-builder": "26.0.10",
    "electron-vite": "3.0.0",
    "eslint": "9.21.0",
    "eslint-plugin-solid": "0.14.5",
    "interactjs": "^1.10.27",
    "piscina": "^5.0.0-alpha.1",
    "postcss": "8.5.3",
    "prettier": "^3.3.2",
    "ractive": "^1.4.4",
    "shx": "^0.3.4",
    "solid-js": "1.9.5",
    "svg-pan-zoom": "^3.6.2",
    "tailwindcss": "^3.4.17",
    "typescript": "5.7.3",
    "vite": "6.2.0",
    "vite-plugin-solid": "2.11.0"
  },
  "overrides": {
    "vite": "$vite"
  },
  "packageManager": "yarn@1.22.22+sha512.a6b2f7906b721bba3d67d4aff083df04dad64c399707841b7acf00f6b133b7ac24255f2652fa22ae3534329dc6180534e98d17432037ff6fd140556e2bb3137e"
}
