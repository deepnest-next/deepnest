{
  "name": "deepnest",
  "version": "1.5.5",
  "description": "Deep nesting for Laser and CNC",
  "private": true,
  "main": "main.js",
  "types": "index.d.ts",
  "license": "MIT",
  "funding": [
    {
      "type": "patreon",
      "url": "https://patreon.com/deepnest_next?utm_medium=clipboard_copy&utm_source=copyLink&utm_campaign=creatorshare_fan&utm_content=join_link"
    }
  ],
  "scripts": {
    "start": "electron .",
    "printenv": "cross-replace node -e \"console.log(process.env)\"",
    "test": "playwright test",
    "build": "tsc && electron-rebuild",
    "clean": "rmdir /s /q build dist >nul 2>&1|echo . >nul",
    "clean-all": "rmdir /s /q build dist node_modules bin >nul 2>&1|echo . >nul",
    "dist": "cross-replace npx @electron/packager . deepnest-v$npm_package_version --overwrite --ignore='\\.github' --ignore='\\.vscode' --ignore='deepnest-v[0-9]+\\.[0-9]+\\.[0-9]+-[a-z0-9_-]' --ignore='out' --ignore='renovate\\.json' --ignore='\\.readthedocs\\.yaml' --ignore='CHANGELOG\\.md' --ignore='examples' --ignore='\\.gitignore'",
    "dist-all": "npm run clean-all && npm install && npm run build && npm run dist",
    "prepare": "husky || true",
    "precommit": "lint-staged"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "**/*.{ts,html,css,scss,less,json}": [
      "prettier --write",
      "eslint --fix",
      "git add"
    ]
  },
  "repository": "https://github.com/deepnest-next/deepnest",
  "keywords": [
    "Electron",
    "Nesting",
    "CNC",
    "Laser"
  ],
  "devDependencies": {
    "@electron/packager": "18.3.6",
    "@electron/rebuild": "4.0.1",
    "@eslint/js": "^9.26.0",
    "@playwright/test": "1.52.0",
    "@types/node": "22.15.16",
    "cross-replace": "0.2.0",
    "electron": "34.5.5",
    "eslint": "^9.26.0",
    "husky": "^9.1.7",
    "lint-staged": "^15.5.2",
    "prettier": "^3.5.3",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.32.0"
  },
  "dependencies": {
    "@deepnest/calculate-nfp": "202503.13.155300",
    "@deepnest/svg-preprocessor": "0.2.0",
    "@electron/remote": "2.1.2",
    "axios": "1.9.0",
    "form-data": "4.0.2",
    "graceful-fs": "4.2.11",
    "marked": "15.0.11"
  },
  "files": [
    "main.js",
    "main/**/*",
    "index.d.ts",
    "node_modules",
    "package.json",
    "icon.icns",
    "icon.ico"
  ],
  "build": {
    "appId": "net.deepnest.app",
    "copyright": "2024 deepnest-next Community",
    "compression": "maximum",
    "nodeGypRebuild": false,
    "mac": {
      "category": "public.app-category.utilities",
      "icon": "icon.icns"
    },
    "win": {
      "icon": "icon.ico"
    }
  },
  "contributors": [
    {
      "name": "Josef Fr√∂hle",
      "email": "github@josef-froehle.de"
    }
  ]
}
